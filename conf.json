{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src", "README.md"],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
      "monospaceLinks": true,
      "cleverLinks": true
  },
  "opts": {
    "template": "./node_modules/fedoc-template",
    "recurse": true,
    "destination": "./doc/",
    "encoding": "utf8"
  }
}
